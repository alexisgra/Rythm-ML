partition JohnnyBGoode

grid {
    bpm 120
    bpb 4
    composition {
        intro
        verse1
        chorus
        verse2
        chorus
    }
}

bar mainBar {
    bd on beat 1 3
    sd on beat 2 4
    rc on quarter 1 2 3 4 5 6 7 8
}

section intro {
    bar 6-13 mainBar
}

section verse1 {
    bar 1-12 mainBar
}

section chorus {
    bar 1-12 mainBar
}

section verse2 {
    bar 1-12 mainBar
}
